#: openbook_auth/models.py:986
#, python-format
msgid "%(post_commenter_name)s · %(post_commenter_username)s commented on your post."
msgstr "%(post_commenter_name)s · %(post_commenter_username)s kommenterade på ditt inlägg."

#: openbook_auth/models.py:993
#, python-format
msgid "%(post_commenter_name)s · @%(post_commenter_username)s commented on a post you also commented on."
msgstr "%(post_commenter_name)s · @%(post_commenter_username)s kommenterade på ett inlägg du också kommenterat på."

#: openbook_auth/models.py:1042
#, python-format
msgid "%(post_commenter_name)s · @%(post_commenter_username)s replied to your comment on a post."
msgstr "%(post_commenter_name)s · @%(post_commenter_username)s svarade på din kommentar på ett inlägg."

#: openbook_auth/models.py:1049
#, python-format
msgid "%(post_commenter_name)s · @%(post_commenter_username)s replied on a comment you also replied on."
msgstr "%(post_commenter_name)s · @%(post_commenter_username)s svarade på en kommentar du också svarat på."

#: openbook_auth/models.py:2119
msgid "A user cannot follow itself."
msgstr "En användare kan inte följa sig själv."

#: openbook_auth/models.py:2188
msgid "A user cannot connect with itself."
msgstr "En användare kan inte knyta kontakt med sig själv."

#: openbook_auth/models.py:2227
msgid "No data to update the connection with."
msgstr "Ingen data att uppdatera kontakten med."

#: openbook_auth/models.py:2608
msgid "You have already reported the moderated_object."
msgstr "Du har redan anmält det modererade objektet."

#: openbook_auth/models.py:2616
msgid "The moderated object has been verified and can no longer be edited."
msgstr "Det modererade objektet har verifierats och kan inte lägre redigeras."

#: openbook_auth/models.py:2621
msgid "The moderated object has already been approved/rejected."
msgstr "Det modererade objektet har redan blivit godkänt/avvisat."

#: openbook_auth/models.py:2629 openbook_auth/models.py:2636
msgid "The moderated object has already been verified."
msgstr "Det modererade objektet har redan blivit verifierat."

#: openbook_auth/models.py:2643
msgid "The moderated object has not been verified."
msgstr "Det modererade objektet har inte verifierats."

#: openbook_auth/models.py:2650
msgid "The moderated object is already verified."
msgstr "Det modererade objektet är redan verifierat."

#: openbook_auth/models.py:2655
msgid "You cannot verify a moderated object with status pending. Please approve or reject it."
msgstr "Du kan inte verifiera ett modererat objekt som har en väntande status. Vänligen godkänn eller avvisa det."

#: openbook_auth/models.py:2672
msgid "Only community staff can moderate community posts"
msgstr "Endast gemenskapspersonal kan moderera gemenskapsinlägg"

#: openbook_auth/models.py:2674
msgid "Only global moderators can moderate non-community posts"
msgstr "Endast globala moderatorer kan moderera inlägg utanför gemenskaper"

#: openbook_auth/models.py:2678
msgid "Only community staff can moderate community post comments"
msgstr "Endast gemenskapspersonal kan moderera kommentarer på gemenskapsinlägg"

#: openbook_auth/models.py:2680
msgid "Only global moderators can moderate non-community post comments"
msgstr "Endast globala moderatorer kan moderera kommentarer på inlägg utanför gemenskaper"

#: openbook_auth/models.py:2682
msgid "Non global moderators can only moderate posts and post comments."
msgstr "Icke-globala moderatorer kan endast moderera inlägg och kommentarer."

#: openbook_auth/models.py:2686
msgid "Not a global moderator."
msgstr "Inte en global moderator."

#: openbook_auth/models.py:2690
msgid "Not a community staff."
msgstr "Inte gemenskapspersonal."

#: openbook_auth/models.py:2694
msgid "You have no invites left"
msgstr "Du har inga inbjudningar kvar"

#: openbook_auth/models.py:2708
msgid "Invite email already sent to this address"
msgstr "E-postinbjudan har redan skickats till den här adressen"

#: openbook_auth/models.py:2718
msgid "Invite is already used and cannot be deleted"
msgstr "Inbjudan har redan använts och kan inte tas bort"

#: openbook_auth/models.py:2723
msgid "Invite was not created by you"
msgstr "Inbjudan skapades inte av dig"

#: openbook_auth/models.py:2729
msgid "Reset your password for Okuna"
msgstr "Återställ ditt lösenord för Okuna"

#: openbook_auth/models.py:2956 openbook_auth/models.py:2997
msgid "Token user id does not match"
msgstr "Kodens användar-id matchar inte"

#: openbook_auth/models.py:2992
msgid "Token email does not match"
msgstr "Kodens e-post matchar inte"

#: openbook_auth/models.py:3026
msgid "Can't connect in the world circle."
msgstr "Kan inte lägga en kontakt i världskretsen."

#: openbook_auth/models.py:3035
msgid "The email is already taken."
msgstr "E-postadressen är redan tagen."

